{
    "version": 2,
    "name": "pullrequest-first",
    "alias": "https://pullrequest-first.now.sh",
    "env": {
        "APP_ID": "34339",
        "WEBHOOK_SECRET": "@webhook-secret",
        "NODE_ENV": "production",
        "PRIVATE_KEY": "@private-key"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/lib/$1"
        }
    ],
    "builds": [
        {
            "src": "./src/index.ts",
            "use": "@now/node"
        }
    ]
}